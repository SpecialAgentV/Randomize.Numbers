//DO-WHILE LOOP

function getRandomNumber(upper) {
  return Math.floor( Math.random() * upper ) + 1;
}
 let counter = 0; 
do{
 console.log(` Random is ${ getRandomNumber(120)}`);
   counter += 1;
          } while (counter <10);
          
